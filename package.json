{
  "name": "binectics",
  "version": "1.0.0",
  "private": true,
  "description": "A global ecosystem connecting gyms, fitness trainers, dieticians, and wellness enthusiasts",
  "scripts": {
    "dev:web": "npm run dev --prefix apps/web",
    "dev:api": "npm run start:dev --prefix apps/api",
    "build:web": "npm run build --prefix apps/web",
    "build:api": "npm run build --prefix apps/api",
    "start:web": "npm run start --prefix apps/web",
    "start:api": "npm run start:prod --prefix apps/api",
    "db:migrate": "npm run --prefix apps/api prisma migrate dev",
    "db:generate": "npm run --prefix apps/api prisma generate"
  },
  "keywords": ["fitness", "gym", "wellness", "marketplace"],
  "license": "MIT"
}
